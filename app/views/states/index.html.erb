<div class="page-header">
  <h1>
    States
    <a href="/states/new" class="btn btn-lg btn-success">New State</a>
  </h1>
</div>

<script src="/vendor/jscolor/jscolor2.js"></script>


<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User:                 <button
                            class="jscolor {valueElement:null,borderColor:'#000', insetColor:'#000', backgroundColor:'#666'}, btn"
                            style="border:2px solid #272727;">
                        </button></th>
        <th>Effect</th>
        <th>Alert</th>
        <th>Blue</th>
        <th>Green</th>
        <th>Red</th>
        <th>Brightness</th>
        <th>On</th>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @states.each do |state| %>
      <tr>
        <td><%= state.user_id %></td>
        <td><%= state.effect %></td>
        <td><%= state.alert %></td>
        <td><%= state.blue %></td>
        <td><%= state.green %></td>
        <td><%= state.red %></td>
        <td><%= state.brightness %></td>
        <td><%= state.on %></td>
        <td><%= state.name %></td>
        <td>
          <a href="/states/<%= state.id %>" class="btn btn-primary">Show</a>
          <a href="/states/<%= state.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_state/<%= state.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
